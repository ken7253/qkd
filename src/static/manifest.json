{
  "manifest_version": 3,
  "version": "0.0.0",
  "short_name": "qkd",
  "name": "quick documents",
  "description": "主にWeb系の技術情報をスピーディーに探す拡張機能です",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "options_ui": {
    "page": "index.html"
  },
  "permissions": [],
  "host_permissions": [
    "<all_urls>"
  ]
}